        <script type="text/javascript">
            function layoutHandler() {
                var style_link_share = document.getElementById("page_style_share");
                var style_link = document.getElementById("page_style");

                if (window.innerWidth < 500) {
                    style_link_share.setAttribute("href", "css/mobile/styles_mobile.css");
                    style_link.setAttribute("href", "css/mobile/contacts_style_mobile.css");
                }
                else {
                    style_link_share.setAttribute("href", "css/regular/styles.css");
                    style_link.setAttribute("href", "css/regular/contacts_style.css");
                }
            }

            window.onresize = layoutHandler;
            layoutHandler();
        </script>
